<ehcache>
    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            maxElementsInMemory="1000"
            eternal="false"
            timeToIdleSeconds="3600"
            timeToLiveSeconds="3600"
            overflowToDisk="true"
            diskPersistent="false"
            diskExpiryThreadIntervalSeconds="8400"
            memoryStoreEvictionPolicy="LRU"
    />

    <cache name="net.driftingsouls.ds2.server.entities.NewsEntry"
    		maxElementsInMemory="50"
    		eternal="true"
    		overflowToDisk="true"
    />

    <cache name="net.driftingsouls.ds2.server.framework.BasicUser"
    		maxElementsInMemory="500"
    		eternal="true"
    		overflowToDisk="true"
    />

    <cache name="net.driftingsouls.ds2.server.battles.BattleShip"
    		maxElementsInMemory="300"
    		eternal="false"
    		overflowToDisk="true"
    />

    <cache name="net.driftingsouls.ds2.server.bases.Base"
    		maxElementsInMemory="500"
    		eternal="false"
    		overflowToDisk="false"
    />

    <cache name="net.driftingsouls.ds2.server.entities.Forschung"
    		maxElementsInMemory="100"
    		eternal="true"
    		overflowToDisk="false"
    />

    <cache name="net.driftingsouls.ds2.server.comm.PM"
    		maxElementsInMemory="500"
    		eternal="false"
    		overflowToDisk="false"
    />

    <cache name="net.driftingsouls.ds2.server.comm.Ordner"
    		maxElementsInMemory="50"
    		eternal="false"
    		overflowToDisk="true"
    />

	<cache  name="net.driftingsouls.ds2.server.entities.Munitionsdefinition"
			maxElementsInMemory="100"
			eternal="true"
			overflowToDisk="true"
	/>
	<cache name="net.driftingsouls.ds2.server.modules.GuiHelpText"
			maxElementsInMemory="300"
			eternal="true"
			overflowToDisk="true"
	/>

	<cache  name="net.driftingsouls.ds2.server.bases.Building"
			maxElementsInMemory="50"
			eternal="true"
			overflowToDisk="false"
  	/>

	<cache  name="net.driftingsouls.ds2.server.bases.Core"
			maxElementsInMemory="20"
			eternal="true"
			overflowToDisk="false"
	/>

	<cache  name="net.driftingsouls.ds2.server.entities.Nebel"
			maxElementsInMemory="200"
			eternal="false"
			timeToIdleSeconds="60"
			timeToLiveSeconds="600"
			overflowToDisk="false"
	/>

	<cache  name="net.driftingsouls.ds2.server.ships.ShipType"
			maxElementsInMemory="300"
			eternal="true"
			overflowToDisk="true"
	/>

	<cache name="net.driftingsouls.ds2.server.entities.FactoryEntry"
			maxElementsInMemory="50"
			eternal="true"
			overflowToDisk="false"
	/>

	<cache  name="net.driftingsouls.ds2.server.units.UnitType"
			maxElementsInMemory="30"
			eternal="true"
			overflowToDisk="false"
  	/>

  	<cache name="net.driftingsouls.ds2.server.entities.KaserneEntry"
			maxElementsInMemory="50"
			eternal="true"
			overflowToDisk="false"
	/>

	<cache name="net.driftingsouls.ds2.server.config.items.Item"
			maxElementsInMemory="500"
			eternal="true"
			overflowToDisk="false" />
</ehcache>